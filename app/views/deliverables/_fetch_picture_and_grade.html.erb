
<div class='team_member_photo'>

  <div class="label">
    <%= fields_for :grade do |grade| %>
        <font color = black> Grade <%= grade.text_field :_for_student ,:name=>member.id,:value => ((grade_obj.nil?)?"":grade_obj.score),:size=> "3", :tabindex=>member.id%>
      <%# if deliverable_type == "individual" %>

        /<%= display_maximum_score %>    </font>

      <%# end %>
    <% end %>
  </div>
  <div style="text-align: center;">
  <a href='http://<%=@hostname_with_port%>/people/<%= member.twiki_name %>' target='_top'>
    <%= image_tag(member.image_uri, :border => 0, :alt => member.human_name+" image") %></a>
  </div>
  <div class='label'><font color = black> <%= member.human_name %></font></div>

  </div>
